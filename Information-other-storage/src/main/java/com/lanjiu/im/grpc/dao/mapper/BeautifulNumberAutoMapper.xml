<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanjiu.im.grpc.dao.mapper.impl.BeautifulNumberAutoMapper">
    <resultMap id="beautifulNumberAutoMap" type="com.lanjiu.im.grpc.pojo.BeautifulNumberAuto">
        <result property="tenMillion" column="ten_million" javaType="java.lang.Integer"></result>
        <result property="oneMillion" column="one_million" javaType="java.lang.Integer"></result>
        <result property="oneHundredThousand" column="one_hundred_thousand" javaType="java.lang.Integer"></result>
        <result property="tenThousand" column="ten_thousand" javaType="java.lang.Integer"></result>
        <result property="oneThousand" column="one_thousand" javaType="java.lang.Integer"></result>
        <result property="oneHundred" column="one_hundred" javaType="java.lang.Integer"></result>
        <result property="ten" column="ten" javaType="java.lang.Integer"></result>
        <result property="one" column="one" javaType="java.lang.Integer"></result>
    </resultMap>
    <select id="selectAllBeautifulNumberAuto" resultMap="beautifulNumberAutoMap">
		select * from beautiful_number_auto
	</select>

    <select id="selectBeautifulNumberAuto" resultMap="beautifulNumberAutoMap">
		select * from beautiful_number_auto where ten_million = #{tenMillion} and one_million = #{oneMillion} and one_hundred_thousand = #{oneHundredThousand} and ten_thousand = #{tenThousand}
		and one_thousand = #{oneThousand} and one_hundred = #{oneHundred} and ten = #{ten} and one = #{one}
	</select>

    <select id="selectBeautifulNumberAutoByThousand" resultMap="beautifulNumberAutoMap">
		select * from beautiful_number_auto where ten_million = #{tenMillion} and one_million = #{oneMillion} and one_hundred_thousand = #{oneHundredThousand} and ten_thousand = #{tenThousand}
		and one_thousand = #{oneThousand}
	</select>

</mapper>