<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanjiu.im.storage.dao.mapper.impl.GuestGroupRegisteredMemberRegisteredUserPictureMapper">
    <resultMap id="guestGroupRegisteredMemberRegisteredUserPictureMap" type="com.lanjiu.im.storage.pojo.GuestGroupRegisteredMemberRegisteredUserPicture">
        <result property="downloadUrl" column="download_url" javaType="java.lang.String"></result>
        <result property="guestGroupId" column="guest_group_id" javaType="java.lang.Integer"></result>
        <result property="registeredMemberId" column="registered_member_id" javaType="java.lang.Integer"></result>
        <result property="registeredUserId" column="registered_user_id" javaType="java.lang.Integer"></result>
        <result property="creationTime" column="creation_time" javaType="java.sql.Date"></result>
    </resultMap>
    <select id="selectAllGuestGroupRegisteredMemberRegisteredUserPicture" resultMap="guestGroupRegisteredMemberRegisteredUserPictureMap">
		select * from guest_group_registered_member_registered_user_picture
	</select>

    <select id="selectGuestGroupRegisteredMemberRegisteredUserPicture" resultMap="guestGroupRegisteredMemberRegisteredUserPictureMap">
		select * from guest_group_registered_member_registered_user_picture where download_url = #{downloadUrl}
	</select>

    <insert id="insertGuestGroupRegisteredMemberRegisteredUserPicture" parameterType="com.lanjiu.im.storage.pojo.GuestGroupRegisteredMemberRegisteredUserPicture">
        insert into guest_group_registered_member_registered_user_picture (download_url, guest_group_id, registered_member_id, registered_user_id, creation_time)
        values (#{downloadUrl,jdbcType=VARCHAR}, #{guestGroupId,jdbcType=INTEGER}, #{registeredMemberId,jdbcType=INTEGER}, #{registeredUserId,jdbcType=INTEGER}, #{creationTime,jdbcType=DATE})
    </insert>

    <delete id="deleteGuestGroupRegisteredMemberRegisteredUserPicture" >
		delete from guest_group_registered_member_registered_user_picture where download_url = #{downloadUrl}
	</delete>
</mapper>