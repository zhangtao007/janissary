        values (#{downloadUrl,jdbcType=VARCHAR}, #{registeredGroupId,jdbcType=INTEGER}, #{guestMemberId,jdbcType=INTEGER}, #{registeredUserId,jdbcType=INTEGER}, #{creationTime,jdbcType=DATE})
    </insert>

    <delete id="deleteRegisteredGroupGuestMemberRegisteredUserPicture" >
		delete from registered_group_guest_member_registered_user_picture where download_url = #{downloadUrl}
	</delete>
</mapper>