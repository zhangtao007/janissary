    <select id="selectAllRegisteredGroupGuestMemberRegisteredUserPicture" resultMap="registeredGroupGuestMemberRegisteredUserPictureMap">
		select * from registered_group_guest_member_registered_user_picture
	</select>

    <select id="selectRegisteredGroupGuestMemberRegisteredUserPicture" resultMap="registeredGroupGuestMemberRegisteredUserPictureMap">
		select * from registered_group_guest_member_registered_user_picture where download_url = #{downloadUrl}
	</select>

